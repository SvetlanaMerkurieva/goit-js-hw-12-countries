<div class="card">
    <h2 class="name-country">{{name}}</h2>
    <div class="description-country">
        <p class="card-text">Capital:{{capital}}</p>
        <p class="card-text">Population:{{population}}</p>
        <p class="card-text">Languages:</p>
        <ul class="list">
            {{#each languages}}
            <li class="list-item">{{Language.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="image">
        <img src="{{flag}}" alt="Flag">
    </div>
</div>